                picture="watchface/market_music_icon.bin"
                highlightPicture="watchface/market_music_blur_icon.bin"
                blendMode="imgSrcOverMode"
                opacity="255"
                pictureX="0"
                pictureY="0"
                rotationAngle="0"
                w="70"
                h="70"
                x="25"
                y="42">btn_music
                <onClick
                        to="C-APP"
                        type="jump" 
                        id="APP_MUSIC">
                </onClick>
        </icon>
        <icon
                picture="watchface/market_light_icon.bin"
                highlightPicture="watchface/market_light_blur_icon.bin"
                blendMode="imgSrcOverMode"
                opacity="255"
                pictureX="0"
                pictureY="0"
                rotationAngle="0"
                w="70"
                h="70"
                x="25"
                y="401">btn_light
                <onClick
                        to="C-APP"
                        type="jump" 
                        id="APP_MUSIC">
                </onClick>
        </icon>
        <icon
                picture="watchface/market_bicycle_icon.bin"
                highlightPicture="watchface/market_bicycle_blur_icon.bin"
                blendMode="imgSrcOverMode"
                opacity="255"
                pictureX="0"
                pictureY="0"
                rotationAngle="0"
                w="70"
                h="70"
                x="315"
                y="401">btn_sport
        </icon>
        <icon
                picture="watchface/market_text_circle_icon.bin"
                highlightPicture="watchface/market_text_circle_blur_icon.bin"
                blendMode="imgSrcOverMode"
                opacity="255"
                pictureX="0"
                pictureY="0"
                rotationAngle="0"
                w="45"
                h="55"
                x="315"
                y="42">btn_step
                <onClick
                        to="C-APP"
                        type="jump" 
                        id="APP_MUSIC">
                </onClick>
                <textbox
                x="7"
                y="25"
                color="white"
                w="55"
                h="49"
                text="97"
                fontSize="24"
                font="watchface/MichromaRegularSize24Bits4Font.bin"
                mode="center">txt_brightness
                </textbox>
        </icon>
        <textbox
                x="0"
                y="152"
                color="white"
                w="410"
                h="49"
                text="SATURDAY"
                fontSize="24"
                font="watchface/MichromaRegularSize24Bits4Font.bin"
                mode="center">txt_week
                <onTime
                    type="MONDAY"
                    to="txt_week"
                    >
                </onTime>
        </textbox>
        <textbox
                x="0"
                y="181"
                color="white"
                w="410"
                h="49"
                text="12/MARCH"
                fontSize="16"
                font="watchface/MichromaRegularSize16Bits4Font.bin"
                mode="center">txt_date
                <onTime
                    type="1/JANUARY"
                    to="txt_date"
                    >
                </onTime>
        </textbox>
    </screen>
    
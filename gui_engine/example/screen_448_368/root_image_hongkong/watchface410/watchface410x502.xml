<screen>
        <img
                x="0"
                y="43"
                opacity="255"
                file="watchface/market_watch_base.bin"
                blendMode="imgBypassMode"
                >watchface
        </img>
        <img
                x="178"
                y="144"
                opacity="255"
                file="watchface_market/watchface1/Union2.bin"
                blendMode="imgSrcOverMode"
                >hour_hand
                <onTime
                    type="hour"
                    to="hour_hand">
                </onTime>
        </img>
        <img
                x="179"
                y="68"
                opacity="255"
                file="watchface_market/watchface1/Union.bin"
                blendMode="imgSrcOverMode"
                >minute_hand
                <onTime
                    type="minute"
                    to="minute_hand">
                </onTime>
        </img>
        <img
                x="185"
                y="90"
                opacity="255"
                file="watchface_market/watchface1/Vector7.bin"
                blendMode="imgSrcOverMode"
                >second_hand
                <onTime
                    type="second"
                    to="second_hand">
                </onTime>
        </img>
        <img
                x="183"
                y="220"
                opacity="255"
                file="watchface_market/watchface1/Ellipse5.bin"
                blendMode="imgSrcOverMode"
                >mask
        </img>
        <icon
                picture="watchface_market/watchface1/Group21.bin"
                highlightPicture="watchface_market/watchface1/Group21hl.bin"
                blendMode="imgSrcOverMode"
                opacity="255"
                pictureX="0"
                pictureY="0"
                rotationAngle="0"
                w="45"
                h="55"
                x="26"
                y="35">btn_music
                <onClick
                        to="app"
                        type="jump" 
                        id="APP_MUSIC">
                </onClick>
        </icon>
        <icon
                picture="watchface_market/watchface1/Group23.bin"
                highlightPicture="watchface_market/watchface1/Group23hl.bin"
                blendMode="imgSrcOverMode"
                opacity="255"
                pictureX="0"
                pictureY="0"
                rotationAngle="0"
                w="45"
                h="55"
                x="26"
                y="358">btn_light
                <onClick
                        to="app"
                        type="jump" 
                        id="APP_MUSIC">
                </onClick>
        </icon>
        <icon
                picture="watchface_market/watchface1/Group22.bin"
                highlightPicture="watchface_market/watchface1/Group22hl.bin"
                blendMode="imgSrcOverMode"
                opacity="255"
                pictureX="0"
                pictureY="0"
                rotationAngle="0"
                w="45"
                h="55"
                x="297"
                y="35">btn_sport
        </icon>
        <icon
                picture="watchface_market/watchface1/Ellipse19.bin"
                highlightPicture="watchface_market/watchface1/Ellipse19hl.bin"
                blendMode="imgSrcOverMode"
                opacity="255"
                pictureX="0"
                pictureY="0"
                rotationAngle="0"
                w="45"
                h="55"
                x="297"
                y="358">btn_step
                <onClick
                        to="app"
                        type="jump" 
                        id="APP_MUSIC">
                </onClick>
                <textbox
                x="0"
                y="15"
                color="white"
                w="55"
                h="49"
                text="97"
                fontSize="24"
                font="watchface_market/watchface1/MichromaRegularSize24Bits4Font.bin"
                mode="center">txt_brightness
                </textbox>
        </icon>
        <textbox
                x="0"
                y="106"
                color="white"
                w="368"
                h="49"
                text="SATURDAY"
                fontSize="24"
                font="watchface_market/watchface1/MichromaRegularSize24Bits4Font.bin"
                mode="center">txt_week
                <onTime
                    type="MONDAY"
                    to="txt_week"
                    >
                </onTime>
        </textbox>
        <textbox
                x="0"
                y="135"
                color="white"
                w="368"
                h="49"
                text="12/MARCH"
                fontSize="16"
                font="watchface_market/watchface1/MichromaRegularSize16Bits4Font.bin"
                mode="center">txt_date
                <onTime
                    type="1/JANUARY"
                    to="txt_date"
                    >
                </onTime>
        </textbox>
    </screen>
    